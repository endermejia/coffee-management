{
  "kind": "collectionType",
  "collectionName": "product_orders",
  "info": {
    "singularName": "product-order",
    "pluralName": "product-orders",
    "displayName": "Product Order",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "quantity": {
      "type": "integer",
      "default": 1,
      "required": true
    },
    "prepared": {
      "type": "boolean"
    },
    "served": {
      "type": "boolean"
    },
    "product": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::product.product"
    },
    "notes": {
      "type": "text"
    },
    "paid": {
      "type": "boolean"
    },
    "order": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::order.order",
      "inversedBy": "product_orders"
    }
  }
}
